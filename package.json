{
  "name": "iamdeveloper-dot-com",
  "description": "My blog, based off of the Netlify CMS Gatsby Starter, https://github.com/netlify-templates/gatsby-starter-netlify-cms.",
  "version": "1.0.0",
  "author": "Nick Taylor",
  "keywords": [
    "gatsby",
    "blog",
    "react",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf .cache public",
    "prebuild": "npm run clean && npm run lint && npm run type-check",
    "build": "gatsby build && cp ./keybase.txt ./public/",
    "predevelop": "npm run clean",
    "develop": "gatsby develop",
    "type-check": "tsc",
    "type-check:watch": "npm run type-check -- --watch",
    "lint:js": "eslint . --ext .ts,.tsx,.js",
    "lint:css": "stylelint src/**/*.scss --config .styletinrc.json",
    "lint": "npm run lint:css && npm run lint:js",
    "serve": "gatsby serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "*.{sass,scss,css}": [
      "stylelint --config .styletinrc.json",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 80
  },
  "dependencies": {
    "@raae/gatsby-remark-oembed": "0.0.3",
    "bulma": "0.7.2",
    "gatsby": "2.0.91",
    "gatsby-image": "2.0.26",
    "gatsby-plugin-feed": "2.0.11",
    "gatsby-plugin-google-analytics": "2.0.9",
    "gatsby-plugin-manifest": "2.0.13",
    "gatsby-plugin-netlify": "2.0.6",
    "gatsby-plugin-netlify-cms": "3.0.9",
    "gatsby-plugin-offline": "2.0.21",
    "gatsby-plugin-purgecss": "3.1.0",
    "gatsby-plugin-react-helmet": "3.0.5",
    "gatsby-plugin-sass": "2.0.7",
    "gatsby-plugin-sharp": "2.0.17",
    "gatsby-plugin-sitemap": "2.0.4",
    "gatsby-plugin-web-font-loader": "1.0.4",
    "gatsby-remark-copy-linked-files": "2.0.8",
    "gatsby-remark-images": "3.0.1",
    "gatsby-remark-prismjs": "3.2.0",
    "gatsby-remark-relative-images": "0.2.1",
    "gatsby-source-filesystem": "2.0.16",
    "gatsby-transformer-remark": "2.2.0",
    "gatsby-transformer-sharp": "2.1.10",
    "lodash": "4.17.11",
    "lodash-webpack-plugin": "0.11.5",
    "netlify-cms": "2.3.3",
    "node-sass": "4.11.0",
    "prismjs": "1.15.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-helmet": "5.2.0"
  },
  "devDependencies": {
    "@types/lodash": "4.14.120",
    "@types/react": "16.7.20",
    "@types/react-helmet": "5.0.8",
    "@types/webpack": "4.4.23",
    "@typescript-eslint/eslint-plugin": "1.0.0",
    "@typescript-eslint/parser": "0.2.1",
    "eslint": "5.12.1",
    "eslint-config-prettier": "3.6.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "gatsby-plugin-typescript": "2.0.3",
    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "postcss-pxtorem": "4.0.1",
    "prettier": "1.16.0",
    "stylelint": "9.10.1",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-scss": "3.5.1",
    "typescript": "3.2.4"
  }
}
